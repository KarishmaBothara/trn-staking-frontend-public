  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 4204",
    "build": "ANALYZE=true next build",
    "start": "next start",
    "test": "jest --watch",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.ts{,x}\"",
    "graphql": "pnpm run generate-schema & pnpm run codegen",
    "generate-schema": "apollo-codegen introspect-schema http://localhost:8080/graphql --output schema.json",
    "codegen": "graphql-codegen --config codegen.yml && prettier --write ./src/generated/graphql.tsx",
    "dotenv:generate": "dotenvi -s ${NODE_ENV:-development}",
    "config:dev": "NODE_ENV=development pnpm run dotenv:generate",
    "config:prod": "NODE_ENV=production pnpm run dotenv:generate"
